{
	"include": [
		"**/*",
	],
	"compilerOptions": {
		"moduleResolution": "Bundler",
		"jsx": "react-jsx",
		"target": "ESNext",
		"useDefineForClassFields": true,
		"module": "ESNext",
		"lib": [
			"ESNext",
			"DOM"
		],
		"types": [
			"vite/client"
		],
		"sourceMap": true,
		"resolveJsonModule": true,
		"noEmit": true,
		"strict": true,
		"noImplicitReturns": true
	}
}